<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="Blando">#424242</Color>
    <Color x:Key="Duro">#0D47A1</Color>
    <Color x:Key="ColorPulsadoM">#1565C0</Color>

    <Color x:Key="BackgroundColor">#1B1B1B</Color>
    <Color x:Key="ForegroundColor">#EDEDED</Color>

    <Color x:Key="VerdeAnadir">#2E7D32</Color>
    <Color x:Key="AmarilloEditar">#F9A825</Color>
    <Color x:Key="RojoBorrar">#B71C1C</Color>

    <SolidColorBrush x:Key="asd" Color="{StaticResource Blando}" />
    <SolidColorBrush x:Key="BordePulsado" Color="{StaticResource Duro}" />
    <SolidColorBrush x:Key="Sin pulsar" Color="{StaticResource ColorPulsadoM}" />

    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}" />
    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource ForegroundColor}" />

    <SolidColorBrush x:Key="Verde" Color="{StaticResource VerdeAnadir}" />
    <SolidColorBrush x:Key="Amarillo" Color="{StaticResource AmarilloEditar}" />
    <SolidColorBrush x:Key="Rojo" Color="{StaticResource RojoBorrar}" />


</ResourceDictionary>